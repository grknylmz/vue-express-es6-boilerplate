{
	"name": "node-express-es6-boilerplate",
	"version": "1.0.0",
	"description": "Boilerplate for Node.js projects with lint, transpile and test configurations.",
	"main": "src/index.js",
	"scripts": {
		"build": "./node_modules/.bin/webpack --config ./build/webpack.api.base.babel.js",
		"clean": "./node_modules/.bin/rimraf ./dist",
		"lint": "./node_modules/.bin/eslint --color --format stylish ./src --fix",
		"prebuild": "npm run clean",
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "nodemon --exec babel-node src/index.js"
	},
	"author": "",
	"keywords": [
		"nodejs",
		"boilerplate",
		"es6",
		"es2015",
		"babel",
		"express",
		"lint"
	],
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.1",
		"eslint": "^6.8.0",
		"eslint-config-standard": "^14.1.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^9.2.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.1.0",
		"file-loader": "^1.1.6",
		"json-loader": "^0.5.7",
		"nodemon": "^2.0.6",
		"rimraf": "^2.6.2",
		"swagger-ui-express": "^4.1.5",
		"webpack": "^3.10.0",
		"webpack-node-externals": "^1.6.0"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"body-parser": "1.18.2",
		"chalk": "^4.1.0",
		"cors": "2.8.4",
		"dotenv": "^8.2.0",
		"express": "4.16.2",
		"mongoose": "^5.10.15",
		"morgan": "^1.10.0",
		"passport": "0.4.0",
		"passport-jwt": "3.0.1",
		"winston": "^3.3.3",
		"winston-daily-rotate-file": "^4.5.0"
	}
}
